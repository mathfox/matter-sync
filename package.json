{
	"name": "@rbxts/matter-sync",
	"version": "0.2.0",
	"description": "Server-client sync library for Matter ECS.",
	"main": "out/init.lua",
	"types": "out/index.d.ts",
	"files": ["out", "!out/examples", "!out/jest.config.*"],
	"scripts": {
		"prepublishOnly": "bun run check && bun run build",
		"build": "rbxtsc --type package",
		"build:tests": "rbxtsc --rojo tests.project.json --type game --noInclude",
		"sourcemap:tests": "rojo sourcemap tests.project.json --output sourcemap.json",
		"check": "bunx @biomejs/biome check --write",
		"serve:tests": "rojo serve tests.project.json",
		"test": "bun run build:tests && rojo build tests.project.json --output tests/tests-place.rbxl && run-in-roblox --script tests/run-tests.luau --place tests/tests-place.rbxl"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mathfox/matter-sync.git"
	},
	"keywords": ["roblox", "roblox-ts", "matter", "sync", "replication"],
	"author": {
		"name": "Ivan Leontev",
		"url": "https://github.com/mathfox"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/mathfox/matter-sync/issues"
	},
	"homepage": "https://github.com/mathfox/matter-sync#readme",
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@rbxts/compiler-types": "^2.3.0-types.2",
		"@rbxts/matter": "^0.8.5",
		"@rbxts/services": "^1.5.5",
		"@rbxts/types": "^1.0.804"
	},
	"devDependencies": {
		"@rbxts/jest": "^0.1.0",
		"@rbxts/jest-globals": "^0.1.0",
		"roblox-ts": "^2.3.0-dev-5532593",
		"typescript": "^5.7.0-dev.20240902"
	}
}
